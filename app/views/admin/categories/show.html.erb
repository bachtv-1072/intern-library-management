<div class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1 class="m-0 text-dark">
          <%= t "categories.show.title" %>
          <%= @category.title %>
        </h1>
      </div>
    </div>
  </div>
</div>
<section class="content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <div class="card">
          <div class="card-body">
            <table id="category" class="table table-striped table-bordered">
              <thead>
                <th><%= t "books.list.column_id" %></th>
                <th><%= t "books.list.column_name" %></th>
                <th><%= t "books.list.column_quantity" %></th>
                <th><%= t "books.list.column_action" %></th>
              </thead>
              <tbody>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
